    <validationRules>
        <fullName>rstk__Minimum_values_required</fullName>
        <active>true</active>
        <description>Minimum values required.</description>
        <errorConditionFormula>OR(ISBLANK(rstk__polineaddr_street__c), ISBLANK(rstk__polineaddr_city__c))</errorConditionFormula>
        <errorMessage>PO Line address requires street and city information.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__poline_not_null</fullName>
        <active>true</active>
        <description>POLine not null.</description>
        <errorConditionFormula>isNULL(rstk__polineaddr_poline__c)</errorConditionFormula>
        <errorMessage>PO Line value needs be set.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__validate_email</fullName>
        <active>true</active>
        <description>Validate Email Address</description>
        <errorConditionFormula>And(NOT(ISBLANK(rstk__polineaddr_contactemail__c)), NOT(REGEX(UPPER(rstk__polineaddr_contactemail__c), &quot;^[A-Z0-9._%+-/!#$%&amp;&apos;*=?^_`{|}~]+@[A-Z0-9.-]+\\.[A-Z]{2,40}$&quot;)))</errorConditionFormula>
        <errorMessage>Email address is not valid.</errorMessage>
    </validationRules>
