    <fields>
        <fullName>Declaration_Periode__c</fullName>
        <externalId>false</externalId>
        <formula>IF ( AND(left(RIGHT(Periode__c, 2),1) &lt;&gt; &quot;Q&quot;,left(RIGHT(Periode__c, 2),1) &lt;&gt; &quot;Y&quot;), Value(RIGHT(Periode__c, 2)), Value(RigHT(Periode__c, 1)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Declaration Periode</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Finalised__c</fullName>
        <externalId>false</externalId>
        <formula>NOT( ISPICKVAL(Status__c, &apos;Open&apos;) )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>This field is used to denote that this Declaration has been finalised.  It is set to TRUE when the status of the Declaration moves beyond Open.</inlineHelpText>
        <label>VAT Declaration Has Been Finalised</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
