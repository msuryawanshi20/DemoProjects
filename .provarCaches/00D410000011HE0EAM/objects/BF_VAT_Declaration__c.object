        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Declaration Clearing Journal Entry</label>
        <referenceTo>rstkf__gljh__c</referenceTo>
        <relationshipLabel>BF VAT Declarations</relationshipLabel>
        <relationshipName>BF_VAT_Declarations</relationshipName>
        <required>false</required>
