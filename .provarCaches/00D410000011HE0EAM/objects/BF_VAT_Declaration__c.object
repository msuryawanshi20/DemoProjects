    <validationRules>
        <fullName>Status_Booked</fullName>
        <active>false</active>
        <description>Vat Declaration booked in GL</description>
        <errorConditionFormula>AND(  ISPICKVAL(PRIORVALUE( Status__c ),&quot;Booked&quot;),  ISCHANGED(Status__c)  )</errorConditionFormula>
        <errorMessage>VAT Declaration is booked, changes are not allowed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Declared</fullName>
        <active>true</active>
        <description>VAT Declaration is Declared</description>
        <errorConditionFormula>AND(  ISPICKVAL(PRIORVALUE( Status__c ),&quot;Declared&quot;), or( ISPICKVAL(Status__c, &quot;Open&quot;), ISPICKVAL(Status__c, &quot;Closed&quot;)))</errorConditionFormula>
        <errorMessage>The VAT Declaration is Declared so you can not reopen it.</errorMessage>
    </validationRules>
