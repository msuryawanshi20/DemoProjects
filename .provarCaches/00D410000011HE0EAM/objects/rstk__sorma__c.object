    <validationRules>
        <fullName>rstk__Adjustment_Account_Reqd_For_Release</fullName>
        <active>true</active>
        <description>Adjustment Account is required in order to release the RMA</description>
        <errorConditionFormula>AND(
 rstk__sorma_released__c = true,
 ISBLANK(rstk__sorma_adjacct__c)
)</errorConditionFormula>
        <errorMessage>Adjustment Account is required in order to release the RMA</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Cannot_Unrelease_When_Qty_Rcvd_Shipped</fullName>
        <active>true</active>
        <description>Cannot unrelease RMA when quantities have been received or shipped</description>
        <errorConditionFormula>AND(
 ISCHANGED(rstk__sorma_released__c),
 rstk__sorma_released__c = false,
 OR(rstk__sorma_qtyrec__c &gt; 0, rstk__sorma_qtyship__c &gt; 0, rstk__sorma_qtyinsp__c &gt; 0, rstk__sorma_qtyscrap__c &gt; 0)
)</errorConditionFormula>
        <errorMessage>Cannot unrelease RMA when quantities have been received or shipped</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Hdr_Release_Reqd_to_Release_Dtl</fullName>
        <active>true</active>
        <errorConditionFormula>rstk__sorma_released__c &amp;&amp; !rstk__sorma_sormh__r.rstk__sormh_released__c</errorConditionFormula>
        <errorMessage>RMA Header must be released in order to release RMA Detail.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__InValid_Location_ID_and_Location_Number</fullName>
        <active>true</active>
        <description>Inventory/Inspection/Scrap location id and location number should be different.</description>
        <errorConditionFormula>rstk__sorma_released__c &amp;&amp; (( rstk__sorma_invlocid__c =  rstk__sorma_insplocid__c &amp;&amp;  rstk__sorma_invlocnum__c =  rstk__sorma_insplocnum__c ) || ( rstk__sorma_invlocid__c =   rstk__sorma_scraplocid__c &amp;&amp;  rstk__sorma_invlocnum__c =   rstk__sorma_scraplocnum__c ) || ( rstk__sorma_scraplocid__c =  rstk__sorma_insplocid__c &amp;&amp;  rstk__sorma_scraplocnum__c =  rstk__sorma_insplocnum__c ))</errorConditionFormula>
        <errorMessage>Inventory/Inspection/Scrap location id and location number should be different.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Inspection_Location_Reqd_For_Release</fullName>
        <active>true</active>
        <description>Inspection location is required in order to release the RMA</description>
        <errorConditionFormula>AND(
 rstk__sorma_released__c = true,
 OR(ISBLANK(rstk__sorma_insplocid__c), ISBLANK(rstk__sorma_insplocnum__c))
)</errorConditionFormula>
        <errorMessage>Inspection location is required in order to release the RMA</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Inventory_Division_Is_Reqd</fullName>
        <active>true</active>
        <description>Inventory division is required</description>
        <errorConditionFormula>ISBLANK(rstk__sorma_invdiv__c)</errorConditionFormula>
        <errorMessage>Inventory division is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Inventory_Location_Reqd_For_Release</fullName>
        <active>true</active>
        <description>Inventory location is required in order to release the RMA</description>
        <errorConditionFormula>AND(
 rstk__sorma_released__c = true,
 OR(ISBLANK(rstk__sorma_invlocid__c), ISBLANK(rstk__sorma_invlocnum__c))
)</errorConditionFormula>
        <errorMessage>Inventory location is required in order to release the RMA</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Issue_Credit_For_Credit_Auth</fullName>
        <active>true</active>
        <description>Action must be Issue Credit in order to authorize a credit memo</description>
        <errorConditionFormula>AND(
 rstk__sorma_creditauth__c = true,
 ISPICKVAL( rstk__sorma_action__c , &apos;Issue Credit&apos;) = false
)</errorConditionFormula>
        <errorMessage>Action must be Issue Credit in order to authorize a credit memo</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Product_Should_Not_Be_Of_Type</fullName>
        <active>true</active>
        <description>Cannot create RMA with Product of type Service or Miscellaneous</description>
        <errorConditionFormula>ISPICKVAL(rstk__sorma_soprod__r.rstk__soprod_prodtype__c, &apos;Service&apos;) || ISPICKVAL(rstk__sorma_soprod__r.rstk__soprod_prodtype__c, &apos;Miscellaneous&apos;)</errorConditionFormula>
        <errorMessage>Cannot create RMA with Product of type Service or Miscellaneous</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Product_once_set_cannot_be_changed</fullName>
        <active>true</active>
        <errorConditionFormula>!ISNEW() &amp;&amp; ISCHANGED( rstk__sorma_soprod__c )</errorConditionFormula>
        <errorMessage>Product once set cannot be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Project_Must_Match_Soline_Project</fullName>
        <active>true</active>
        <description>When soline is specified, Project must match soline project</description>
        <errorConditionFormula>AND(
(NOT ISBLANK( rstk__sorma_soline__r.rstk__soline_project__c )),
(rstk__sorma_soline__r.rstk__soline_project__c !=  rstk__sorma_project__c) 
)</errorConditionFormula>
        <errorMessage>When soline is specified, Project must match soline project</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Qty_Authorized_cannot_exceed_Qty_Shipped</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISPICKVAL(rstk__sorma_soshipline__r.rstk__soshipline_prod__r.rstk__soprod_prodtype__c , &apos;Kit&apos;)) &amp;&amp; 
((!ISBLANK(rstk__sorma_soshipline__c) &amp;&amp; rstk__sorma_qtyauth__c &gt; rstk__sorma_soshipline__r.rstk__soshipline_qtyship__c) || (!ISBLANK(rstk__sorma_soline__c) &amp;&amp; rstk__sorma_qtyauth__c &gt; rstk__sorma_soline__r.rstk__soline_qtyship__c))</errorConditionFormula>
        <errorMessage>Qty Authorized cannot exceed Qty Shipped</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Qty_Received_cannot_exceed_Qty_Auth</fullName>
        <active>true</active>
        <description>Qty Received cannot exceed Qty Authorized</description>
        <errorConditionFormula>rstk__sorma_qtyrec__c &gt;  rstk__sorma_qtyauth__c</errorConditionFormula>
        <errorMessage>Qty Received cannot exceed Qty Authorized</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Qty_in_Inspection_cannot_exceed_Qty_Auth</fullName>
        <active>true</active>
        <description>Qty in Inspection cannot exceed Qty Authorized</description>
        <errorConditionFormula>rstk__sorma_qtyinsp__c &gt;  rstk__sorma_qtyauth__c</errorConditionFormula>
        <errorMessage>Qty in Inspection cannot exceed Qty Authorized</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Quantity_Authorized_Greaterthanzero</fullName>
        <active>true</active>
        <description>Quantity Authorized must be greater than zero</description>
        <errorConditionFormula>OR(ISNULL(rstk__sorma_qtyauth__c),  rstk__sorma_qtyauth__c &lt;= 0)</errorConditionFormula>
        <errorMessage>Quantity Authorized must be greater than zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Scrap_Location_Reqd_For_Release</fullName>
        <active>true</active>
        <description>Scrap location is required in order to release the RMA</description>
        <errorConditionFormula>AND(
 rstk__sorma_released__c = true,
 OR(ISBLANK(rstk__sorma_scraplocid__c), ISBLANK(rstk__sorma_scraplocnum__c))
)</errorConditionFormula>
        <errorMessage>Scrap location is required in order to release the RMA</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Soline_Inventory_DIvision_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>!ISBLANK( rstk__sorma_soline__c ) &amp;&amp;  rstk__sorma_soline__r.rstk__soline_shipdiv__c !=  rstk__sorma_invdiv__c</errorConditionFormula>
        <errorMessage>The Inventory Division must match the Inventory Division for the Sales Order Line</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Sorma_date_required</fullName>
        <active>true</active>
        <description>Date Issued is required</description>
        <errorConditionFormula>ISBLANK(rstk__sorma_date__c)</errorConditionFormula>
        <errorMessage>Date Issued is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Shipper_Line</fullName>
        <active>true</active>
        <description>Validate Shipper Line</description>
        <errorConditionFormula>!ISBLANK(rstk__sorma_soshipline__c) &amp;&amp; (rstk__sorma_soshipline__r.rstk__soshipline_line__c != rstk__sorma_soline__c || rstk__sorma_soshipline__r.rstk__soshipline_qtyship__c &lt;=0)</errorConditionFormula>
        <errorMessage>Shipper Line is not valid for the specified Sales Order Line</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__sorma_dracct_validation</fullName>
        <active>true</active>
        <description>RMA Receipt Debit Account should belong to the RMA Inventory Division and should be of type Inventory, Inventory Adjustment, Non Nettable, OTH Clearing, Other.</description>
        <errorConditionFormula>!ISBLANK(rstk__sorma_dracct__c) &amp;&amp; (
rstk__sorma_dracct__r.rstk__syacc_div__c != rstk__sorma_invdiv__c || !( ISPICKVAL(rstk__sorma_dracct__r.rstk__syacc_gltype_pl__c, &apos;Inventory&apos;) ||
ISPICKVAL(rstk__sorma_dracct__r.rstk__syacc_gltype_pl__c, &apos;Inventory Adjustment&apos;) ||
ISPICKVAL(rstk__sorma_dracct__r.rstk__syacc_gltype_pl__c, &apos;Non Nettable&apos;) ||
ISPICKVAL(rstk__sorma_dracct__r.rstk__syacc_gltype_pl__c, &apos;OTH Clearing&apos;) ||
ISPICKVAL(rstk__sorma_dracct__r.rstk__syacc_gltype_pl__c, &apos;Other&apos;)
))</errorConditionFormula>
        <errorMessage>RMA Receipt Debit Account should belong to the RMA Inventory Division and should be of type Inventory, Inventory Adjustment, Non Nettable, OTH Clearing, Other.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__sorma_installaddr_validation</fullName>
        <active>true</active>
        <description>Installation Address is required. It should belong to the Installation Customer and should be a valid Installation Address</description>
        <errorConditionFormula>ISBLANK(rstk__sorma_installaddr__c) || rstk__sorma_installaddr__r.rstk__socaddr_custno__c != rstk__sorma_installcust__c || rstk__sorma_installaddr__r.rstk__socaddr_useasinstall__c = FALSE</errorConditionFormula>
        <errorMessage>Installation Address is required. It should belong to the Installation Customer and should be a valid Installation Address</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__sorma_line_validation</fullName>
        <active>true</active>
        <description>Line is required , should be unique under RMA Header and cannot me modified in edit mode.</description>
        <errorConditionFormula>ISBLANK(rstk__sorma_line__c) || (! ISNEW() &amp;&amp;  ISCHANGED(rstk__sorma_line__c) )</errorConditionFormula>
        <errorMessage>Line is required , should be unique under RMA Header and cannot me modified in edit mode.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__sorma_reason_validation</fullName>
        <active>true</active>
        <description>Reason for Return is invalid. It should have the Use For RMA Returns checked and should have the division blank or point to the RMA Header Division</description>
        <errorConditionFormula>!ISBLANK(rstk__sorma_reason__c) &amp;&amp; (
!rstk__sorma_reason__r.rstk__soreason_rma__c || (!ISBLANK(rstk__sorma_reason__r.rstk__soreason_div__c) &amp;&amp; rstk__sorma_reason__r.rstk__soreason_div__c != rstk__sorma_sormh__r.rstk__sormh_div__c))</errorConditionFormula>
        <errorMessage>Reason for Return is invalid. It should have the Use For RMA Returns checked and should have the division blank or point to the RMA Header Division</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__sorma_released_changed</fullName>
        <active>true</active>
        <description>One or more fields updated is non editable after the RMA is released.</description>
        <errorConditionFormula>!ISNEW() &amp;&amp;  PRIORVALUE(rstk__sorma_released__c) == true &amp;&amp; (rstk__sorma_qtyrec__c &gt; 0 || rstk__sorma_qtyship__c &gt; 0 || rstk__sorma_qtyinsp__c &gt; 0 || rstk__sorma_qtyscrap__c &gt; 0) &amp;&amp; 
(ISCHANGED(rstk__sorma_released__c) || ISCHANGED(rstk__sorma_qtyauth__c) || ISCHANGED(rstk__sorma_soline__c) || ISCHANGED(rstk__sorma_invdiv__c) || ISCHANGED(rstk__sorma_project__c) || ISCHANGED(rstk__sorma_adjacct__c) || ISCHANGED(rstk__sorma_wbs__c) || ISCHANGED(rstk__sorma_site__c) || ISCHANGED(rstk__sorma_insplocid__c) || ISCHANGED(rstk__sorma_invlocid__c) ||  ISCHANGED(rstk__sorma_scraplocid__c) || ISCHANGED(rstk__sorma_insplocnum__c) || ISCHANGED(rstk__sorma_invlocnum__c) || ISCHANGED(rstk__sorma_scraplocnum__c) || ISCHANGED(rstk__sorma_insptempl__c) || ISCHANGED(rstk__sorma_case__c))</errorConditionFormula>
        <errorMessage>One or more fields updated is non editable after the RMA is released.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__sorma_resp_validation</fullName>
        <active>true</active>
        <description>Responsible User should not point to an Obsolete User</description>
        <errorConditionFormula>!ISBLANK(rstk__sorma_resp__c) &amp;&amp; rstk__sorma_resp__r.rstk__syusr_obsolete__c</errorConditionFormula>
        <errorMessage>Responsible User should not point to an Obsolete User</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__sorma_shipcost_validation</fullName>
        <active>true</active>
        <description>Shipping Costs is required and cannot be less than zero.</description>
        <errorConditionFormula>!ISBLANK(rstk__sorma_shipcost__c) &amp;&amp; rstk__sorma_shipcost__c &lt; 0</errorConditionFormula>
        <errorMessage>Shipping Costs is required and cannot be less than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__sorma_site_validation</fullName>
        <active>true</active>
        <description>Receive to Site is required. It should belong to the Inventory Division.</description>
        <errorConditionFormula>ISBLANK(rstk__sorma_site__c) || rstk__sorma_site__r.rstk__sysite_div__c != rstk__sorma_invdiv__c</errorConditionFormula>
