    <validationRules>
        <fullName>rstk__Minimum_values_required</fullName>
        <active>true</active>
        <description>Minimum values required.</description>
        <errorConditionFormula>OR(ISBLANK(rstk__pohdraddr_street__c), ISBLANK(rstk__pohdraddr_city__c))</errorConditionFormula>
        <errorMessage>PO header address requires street, city information</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Type</fullName>
        <active>true</active>
        <description>Validate Type.</description>
        <errorConditionFormula>NOT(
OR(
ISPICKVAL(rstk__pohdraddr_type__c , &quot;Billing&quot;),
ISPICKVAL(rstk__pohdraddr_type__c , &quot;PO Mail To&quot;),
ISPICKVAL(rstk__pohdraddr_type__c , &quot;Shipment&quot;))
)</errorConditionFormula>
        <errorMessage>Address Type has to be one of Billing, PO Mail To, Shipment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__pohdr_not_null</fullName>
        <active>true</active>
        <description>PO Header not null.</description>
        <errorConditionFormula>isNULL(rstk__pohdraddr_ordno__c)</errorConditionFormula>
        <errorMessage>Purchase Order Header needs be specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__validate_email</fullName>
        <active>true</active>
        <description>Validate Email</description>
        <errorConditionFormula>And(NOT(ISBLANK(rstk__pohdraddr_contactemail__c)), NOT(REGEX(UPPER(rstk__pohdraddr_contactemail__c), &quot;^[A-Z0-9._%+-/!#$%&amp;&apos;*=?^_`{|}~]+@[A-Z0-9.-]+\\.[A-Z]{2,40}$&quot;)))</errorConditionFormula>
        <errorMessage>Email address is not valid.</errorMessage>
    </validationRules>
