        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Create Sales Invoices</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/28.0/connection.js&quot;)} 

var result = sforce.connection.describeGlobal();
var sobjects = result.getArray(&quot;sobjects&quot;);
if (sobjects.length == 0) { sobjects = result.getArray(&quot;types&quot;); }
var re = /([A-Za-z0-9]+)?(__)?(icixr__c)/;
var prefix = &apos;&apos;;
for (var i=0; i&lt;sobjects.length; i++) {
	var mData = re.exec(sobjects[i]);
	if (mData ==null) continue;
	if (mData[3] == &apos;icixr__c&apos;) {
		prefix = mData[1]==null?&apos;&apos;:(mData[1]+mData[2]);
		break;
	}
}
sforce.connection.defaultNamespace = prefix;

function clearWaiting() {
  var titles = document.getElementsByClassName(&apos;mainTitle&apos;);
  if (titles.length &gt; 0) {
    while (titles[0].firstChild) {
      titles[0].removeChild(titles[0].firstChild);
    }
  }
  var btns = document.getElementsByClassName(&apos;btn&apos;)
  for (var i=0;i&lt;btns.length; i++) { btns[i].disabled=false; }
}

function setWaiting() {
  var titles = document.getElementsByClassName(&apos;mainTitle&apos;);
  if (titles.length &gt; 0) {
    var domText = document.createTextNode(&quot;          &quot;);
    var domImg = document.createElement(&quot;img&quot;);
    domImg.src = &quot;/img/loading32.gif&quot;;
    titles[0].appendChild(domText);
    titles[0].appendChild(domImg);
  }

  var btns = document.getElementsByClassName(&apos;btn&apos;)
  for (var i=0;i&lt;btns.length; i++) { btns[i].disabled=true; }
}

function checkStatus(transId) {
	var results = sforce.connection.query(&quot;Select Id, Name, sydata_txntype__c, sydata_message__c, sydata_process__c From sydata__c where Id = &apos;&quot; + transId + &quot;&apos;&quot;).getArray(&quot;records&quot;);
  if (results[0].sydata_process__c == &apos;Ready&apos;) {
	setTimeout(function() { checkStatus(transId); }, 2000);
  } else {
	if (results[0].sydata_message__c != null) {
	  	clearWaiting();
		alert(results[0].sydata_message__c);
	} else {
		location.reload();
	}
  }
}

function createObject(o) {
  var result = sforce.connection.create([o]);

  if (result[0].getBoolean(&quot;success&quot;)) {
  	checkStatus(result[0].id);
  } else {
  	clearWaiting();
  	alert(&apos;Transaction Failed: &apos; + result[0]);
  }
}

var soconpbbatch =
    sforce.connection.query(
        &quot;select soconpbbatch_soibatch__c &quot; +
        &quot;from soconpbbatch__c &quot; +
        &quot;where Id=&apos;{!rstk__soconpbbatch__c.Id}&apos; &quot; +
        &quot;and soconpbbatch_soibatch__c != null&quot;)
    .getArray(&quot;records&quot;);
if (soconpbbatch.length &gt; 0) {
  alert(&apos;The Sales Order Invoice Batch has already been created.&apos;);
} else {
var sydata = new sforce.SObject(&quot;sydata__c&quot;);

sydata.sydata_txntype__c = &apos;Create SOINV from SOCONPBILL&apos;;
sydata.sydata_soconpbbatchid__c = &apos;{!rstk__soconpbbatch__c.Id}&apos;;
sydata.sydata_backgroundprocessing__c = true;
setWaiting();

setTimeout(function() { createObject(sydata); }, 200);
}</url>
