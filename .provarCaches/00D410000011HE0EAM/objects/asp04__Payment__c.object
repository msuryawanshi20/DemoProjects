    <validationRules>
        <fullName>asp04__PreventFromNegativeAmountEntry</fullName>
        <active>true</active>
        <description>Any negative values entered in amount on payment records are not accepted. Zero amounts are allowed on payment records.</description>
        <errorConditionFormula>asp04__Amount__c &lt; 0</errorConditionFormula>
        <errorDisplayField>asp04__Amount__c</errorDisplayField>
        <errorMessage>Amount cannot be negative.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>asp04__Protect_submitted_data</fullName>
        <active>true</active>
        <description>Stop changes to the dates, amounts and payment route when the Status indicates that data has been sent to Asperato but no response has been recieved yet.</description>
        <errorConditionFormula>AND( 
ISPICKVAL(PRIORVALUE(asp04__Payment_Stage__c),&apos;Submitted for collection&apos;), 
OR(ISCHANGED(asp04__Amount__c) , ISCHANGED(asp04__Due_Date__c) ,  ISCHANGED(asp04__Payment_Route_Selected__c)),
ISPICKVAL(asp04__Payment_Stage__c, &apos;Submitted for collection&apos;)  
)</errorConditionFormula>
        <errorMessage>You can&apos;t change the Amount, Due Date or the Payment Route Selected while Asperato is processing the payment</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>asp04__SetupAsperatoOneValidation</fullName>
        <active>true</active>
        <description>Setup Asperato ONE before creating a payment.</description>
        <errorConditionFormula>ISNEW() &amp;&amp;   $Setup.asp04__AsperatoOneSettings__c.asp04__Customer_ID__c == &apos;&apos;</errorConditionFormula>
        <errorMessage>You must setup Asperato ONE before creating a payment.</errorMessage>
    </validationRules>
